<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="100.0" prefWidth="1120.0" style="-fx-background-color: #323232; -fx-background-radius: 20;" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ensa.project_vt.SousFrame1Controller">
   <children>
      <Label fx:id="videoTitle" alignment="TOP_LEFT" layoutX="41.0" layoutY="12.0" prefHeight="32.0" prefWidth="430.0" text="Cat video with subtitles" textFill="WHITE">
         <font>
            <Font name="System Bold" size="22.0" />
         </font>
      </Label>
      <Label fx:id="subNotsub" alignment="CENTER" layoutX="41.0" layoutY="56.0" prefHeight="25.0" prefWidth="79.0" style="-fx-background-color: #0fc1a1; -fx-background-radius: 25;" text="Sub/Notsub" />
      <Label fx:id="date" alignment="CENTER" layoutX="155.0" layoutY="59.0" prefHeight="20.0" prefWidth="119.0" text="dd/mm/yyyy" textFill="#e1e1e1">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Button fx:id="playV" layoutX="1024.0" layoutY="17.0" mnemonicParsing="false" onAction="#PlayVideo" prefHeight="64.0" prefWidth="55.0" style="-fx-background-color: #323232;">
         <graphic>
            <ImageView fitHeight="60.0" fitWidth="55.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@PlayButtonCircled.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
      <Button fx:id="deleteV" layoutX="956.0" layoutY="20.0" mnemonicParsing="false" onAction="#DeleteVideo" prefHeight="59.0" prefWidth="55.0" style="-fx-background-color: #323232;">
         <graphic>
            <ImageView fitHeight="39.0" fitWidth="39.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@Trash.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
      <Button fx:id="subtitlesV" layoutX="879.0" layoutY="27.0" mnemonicParsing="false" onAction="#getSubtitles" style="-fx-background-color: #323232;">
         <graphic>
            <ImageView fitHeight="39.0" fitWidth="39.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@downloadFile%201.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
   </children>
</AnchorPane>
